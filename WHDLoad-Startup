; $VER: WHDLoad-Startup 1.0 2024-01-11

.KEY L/K,C/K,I/K
.BRA <
.KET >

SET PLIST ""
SET PINFO ""
IF NOT "<L>" EQ ""
  SET PLIST "LIST <L>"
ENDIF
IF NOT "<I>" EQ ""
  SET PINFO "INFO <I>"
  ECHO "WHDLoad-Startup Called by file: <I>"
ENDIF

IF "<C>" EQ ""
  REXXC:RX REXX:ABR.rexx FIRSTMATCH $PLIST CONFIG WHDLoadStartup $PINFO
ELSE
  REXXC:RX REXX:ABR.rexx FIRSTMATCH $PLIST CONFIG <C> $PINFO
ENDIF

IF NOT VAL $RC EQ 0
  EXIT $RC
ENDIF
